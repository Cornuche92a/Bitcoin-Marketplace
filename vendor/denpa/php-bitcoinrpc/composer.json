{
    "name": "denpa/php-bitcoinrpc",
    "type": "library",
    "description": "Bitcoin JSON-RPC client based on GuzzleHttp",
    "keywords": ["bitcoin","api","jsonrpc","guzzle"],
    "homepage": "https://github.com/denpamusic/php-bitcoinrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Paavilainen",
            "email": "denpa@netfleet.space",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.5 | ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 | ^8.0"
    },
    "autoload": {
        "psr-4" : {
            "Denpa\\Bitcoin\\": "src"
        },
        "files" : [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4" : {
            "Denpa\\Bitcoin\\Tests\\": "tests"
        }
    }
}
